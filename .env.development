# Development Environment Variables
NODE_ENV=development
PORT=3000
API_PREFIX=/api

# Database Configuration
DB_TYPE=memory
# DB_TYPE=oracle
# DB_HOST=localhost
# DB_PORT=1521
# DB_NAME=todolist
# DB_USER=todolist_user
# DB_PASSWORD=your_password

# CORS Configuration
CORS_ORIGIN=*

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info

# Security
REQUEST_SIZE_LIMIT=10mb
